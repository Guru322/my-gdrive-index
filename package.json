{
    "name": "gdrive-index",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "server": "node server/server.js",
        "bot:discord": "node -e \"require('./server/discord').startBot()\"",
        "bot:telegram": "python server/telegram.py",
        "dev": "concurrently \"npm run start\" \"npm run server\"",
        "dev:bot": "concurrently \"npm run bot:discord\" \"npm run bot:telegram\"",
        "dev:all": "concurrently \"npm run start\" \"npm run server\" \"npm run bot:discord\" \"npm run bot:telegram\"",
        "prod": "npm run build && concurrently \"npm run server\" \"npm run bot:discord\" \"npm run bot:telegram\""
    },
    "dependencies": {
        "@discordjs/rest": "^1.7.0",
        "@emotion/react": "^11.10.0",
        "@emotion/styled": "^11.10.0",
        "@mui/material": "^5.13.0",
        "cors": "^2.8.5",
        "discord-api-types": "^0.37.40",
        "discord.js": "^14.11.0",
        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "node-telegram-bot-api": "^0.61.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.11.0",
        "react-scripts": "^5.0.1"
    },
    "devDependencies": {
        "concurrently": "^8.0.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "http://localhost:3005"
}
